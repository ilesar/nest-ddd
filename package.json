{
  "name": "petarpetru",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "-- PROJECT --": "",
    "bfjs:build": "npm run build",
    "bfjs:watch": "npm run start:dev",
    "bfjs:debug": "npm run start:debug",
    "-- QUALITY --": "",
    "bfjs:lint": "npm run lint:fix",
    "bfjs:fences": "npm run good:fences",
    "bfjs:e2e": "DISABLE_AUTO_SCHEMA=true npm run test:e2e",
    "bfjs:load": "npm run k6",
    "-- DATABASE --": "",
    "bfjs:reset": "npm run db:recreate",
    "bfjs:fixtures": "npm run fixtures:load",
    "bfjs:seed": "npm run seeder:seed",
    "bfjs:drop": "npm run db:drop",
    "-- PIPELINES --": "",
    "bfjs:ci": "npm run bfjs:build && npm run good:fences && npm run lint",
    "-- TOOLS --": "",
    "bfjs:generate": "nest g -c @bornfight/bfjs-nest-schematics resource",
    "bfjs:uml": "npm run erd",
    "-- DEFAULT COMMANDS --": "",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "-- ADDON COMMANDS --": "",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:migration-fix": "eslint \"src/_shared/infrastructure/modules/database/migrations/*.ts\" --fix",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js --config src/_shared/infrastructure/config/database.config.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d src/_shared/infrastructure/modules/database/migrations",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "db:drop": "npx typeorm -f dist/_shared/infrastructure/config/database.config.js schema:drop",
    "fixtures:load": "npx fixtures -d src/_shared/infrastructure/modules/database/fixtures -c dist/_shared/infrastructure/config/database.config.js --require ts-node/register --require=tsconfig-paths/register",
    "fixtures:load_new": "npx fixtures dist/_shared/infrastructure/modules/database/fixtures -c dist/_shared/infrastructure/config/database.config.js",
    "seeder:seed": "node dist/seed.js",
    "db:recreate": "npm run db:drop && npm run migration:run && npm run seeder:seed && npm run fixtures:load",
    "good:fences": "gf",
    "ci:flow": "npm run build && npm run good:fences",
    "erd": "typeorm-uml dist/_shared/infrastructure/config/database.config.js --download ./../../../../public/erd.png",
    "tools:uml:svg": "tsuml2 --glob src/_shared/domain/models/**/*.ts --tsconfig ./tsconfig.json --outFile ./public/vector/uml.svg",
    "tools:uml:png": "tsuml2 --glob src/_shared/domain/models/*.ts --tsconfig ./tsconfig.json --outFile ./public/images/uml.png",
    "prepare": "husky install",
    "deletemigrations": "rm -rf ./src/_shared/infrastructure/modules/database/migrations/*",
    "k6": "k6 run ./test/load/judgeDemoEvent.js"
  },
  "dependencies": {
    "@adminjs/express": "^4.0.1",
    "@adminjs/nestjs": "^3.0.1",
    "@adminjs/typeorm": "^2.0.0",
    "@codebrew/nestjs-storage": "^0.1.5",
    "@nest-toolbox/winston-logger": "^1.4.4",
    "@nestjs-architects/typed-cqrs": "^0.1.1",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs-query/query-graphql": "^0.30.0",
    "@nestjs-query/query-typeorm": "^0.30.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.2.3",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@ntegral/nestjs-sentry": "^3.0.6",
    "@sentry/node": "^6.16.0",
    "@slynova/flydrive": "^1.0.3",
    "adminjs": "^5.3.3",
    "apollo-server-express": "^3.5.0",
    "argon2": "^0.28.3",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "graphql": "^15.8.0",
    "graphql-relay": "^0.9.0",
    "graphql-subscriptions": "^2.0.0",
    "inquirer": "^8.2.0",
    "jwks-rsa": "^2.0.5",
    "luxon": "^2.1.1",
    "nest-status-monitor": "^0.1.4",
    "nestjs-dataloader": "^7.0.1",
    "nestjs-enlighten": "0.0.8-beta",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@bornfight/bfjs-nest-schematics": "0.0.16",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "file:./../../../Work/elevien/bfjs-nest-schematics",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/eslint": "^7.29.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/luxon": "^1.26.1",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "good-fences": "^0.9.2",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-morph": "^13.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typeorm-fixtures-cli": "^1.9.1",
    "typeorm-uml": "^1.6.4",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleNameMapper": {
      "@root/(.*)$": "<rootDir>/$1",
      "@core/(.*)$": "<rootDir>/core/$1",
      "@shared/(.*)$": "<rootDir>/shared/$1",
      "@user/(.*)$": "<rootDir>/user@/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/(domain|application)/**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@core": "dist/core",
    "@user": "dist/user"
  }
}
